{
  "version": 2,
  "builds": [
    {
      "src": "apps/api/api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/apps/api/api/$1"
    }
  ],
  "functions": {
    "apps/api/api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
